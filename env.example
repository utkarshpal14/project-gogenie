# GoGinie Environment Variables Template
# Copy this file to .env.local and fill in your actual API keys

# AI API Configuration - Choose your preferred free API
# OpenAI (Free tier: $5/month)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude (Free tier: $5/month)
VITE_CLAUDE_API_KEY=your_claude_api_key_here

# Google Gemini (Free tier: 15 requests/minute)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Hugging Face (Free tier: 30k requests/month)
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Cohere (Free tier: 5 requests/minute)
VITE_COHERE_API_KEY=your_cohere_api_key_here

# Perplexity (Free tier: 5 requests/minute)
VITE_PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Google Maps API Configuration
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Travel APIs (Future Phase 3)
VITE_AMADEUS_API_KEY=your_amadeus_api_key_here
VITE_AMADEUS_API_SECRET=your_amadeus_api_secret_here

# Payment Processing (Future Phase 3)
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Weather API (Future Phase 3)
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Database Configuration (Future Backend)
DATABASE_URL=your_database_url_here

# Application Configuration
VITE_APP_NAME=GoGinie
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=http://localhost:3000/api

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_BOOKING_FEATURES=false
VITE_ENABLE_PAYMENT_FEATURES=false

# Development Configuration
NODE_ENV=development
VITE_DEBUG_MODE=true 